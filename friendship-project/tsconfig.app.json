{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/*.vue",
    "../models",
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "types": [
      "vite/client"
    ],
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "~src/*": [
        "./src/*"
      ],
      "utils": [
        "./src/utils"
      ],
      "~utils/*": [
        "./src/utils/*"
      ],
      "components": [
        "./src/components"
      ],
    }
  }
}